/* Visit https://aka.ms/tsconfig to read more about this file */
{
  "compilerOptions": {
    "target": "es6",
    "lib": ["ES6"],
    "module": "commonjs",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
